{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Tunnel": {
    "Url": "https://localhost:7244/connect-ws?host=backend1.app"
  }, 
  "ReverseProxy": {
    "Routes": {
      "route1": {
        "ClusterId": "cluster1",
        "Match": {
          "Path": "{**catch-all}"
        }
      }
    },
    "Clusters": {
      "cluster1": {
        "Destinations": {
          "cluster1/destination1": {
            //"Address": "https://localhost:7053/"
            //"Address": "https://azure.microsoft.com"
            "Address": "http://localhost"
          }
        }
      }
    }
  }
}
